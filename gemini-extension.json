{
    "name": "gemini-prompts",
    "version": "1.3.2",
    "description": "Hot-reloadable prompts with chains, gates, and symbolic syntax (>>prompt --> >>chain @framework :: 'gate')",
    "main": "GEMINI.md",
    "mcpServers": {
        "gemini-prompts": {
            "command": "npx",
            "args": ["claude-prompts", "--transport=stdio"],
            "env": {
                "MCP_WORKSPACE": "${extensionPath}${/}node_modules${/}claude-prompts",
                "MCP_RESOURCES_PATH": "${extensionPath}${/}node_modules${/}claude-prompts${/}resources"
            }
        }
    }
}
