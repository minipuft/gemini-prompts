{
    "name": "gemini-prompts",
    "version": "1.2.0",
    "description": "Hot-reloadable prompts with chains, gates, and symbolic syntax (>>prompt --> >>chain @framework :: 'gate')",
    "main": "GEMINI.md",
    "mcpServers": {
        "gemini-prompts": {
            "command": "node",
            "args": ["${extensionPath}${/}server${/}dist${/}index.js", "--transport=stdio"],
            "env": {
                "MCP_WORKSPACE": "${extensionPath}",
                "MCP_RESOURCES_PATH": "${extensionPath}${/}server${/}resources"
            }
        }
    }
}
